@using Sozluk.EntityLayer.Concrete;
@model List<Category>

<table class="table table-bordered">
	<tr>
		<th>ID</th>
		<th>Ad</th>
		<th>Açıklama</th>
		<th>Durum</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>@item.Id</td>
			<td>@item.Name</td>
			<td>@item.Description</td>
			<td>@item.Status</td>
		</tr>
	}
</table>
